{
    "name": "DryDeploy",
    "dependencies": {
        "win32nt": {
            "desktop": {
                "git": {
                },
                "nuget": {
                    "sources": [
                        {
                            "Name": "PSGallery",
                            "InstallationPolicy": "Trusted",
                            "SourceLocation": "https://www.powershellgallery.com/api/v2"
                        }
                    ],
                    "modules": [
                        {
                            "Name": "PSnmap",
                            "MinimumVersion": "1.3.1"
                        },
                        {
                            "Name": "Posh-SSH",
                            "MinimumVersion": "2.2"
                        },
                        {
                            "Name": "Posh-Git", 
                            "MinimumVersion": "1.0.0"
                        },
                        {
                            "Name": "Foil",
                            "MinimumVersion": "0.1.0"
                        },
                        {
                            "Name": "PowershellGet",
                            "MinimumVersion": "2.2.5"
                        },
                        {
                            "Name": "GPRegistryPolicyParser",
                            "MinimumVersion": "1.0.0"
                        }
                    ]
                },
                "choco": {
                    "sources": [
                        {
                            "Name": "Chocolatey",
                            "InstallationPolicy": "Trusted",
                            "SourceLocation": "https://chocolatey.org/api/v2/"
                        }
                    ],
                    "packages": [
                        {
                            "Name": "Git",
                            "Description": "Git client"
                        },
                        {
                            "Name": "Packer",
                            "Description": "Hashicorp Packer"
                        },
                        {
                            "Name": "Terraform",
                            "Description": "Hashicorp Terraform"
                        },
                        {
                            "Name": "windows-adk-oscdimg",
                            "Description": "Windows ADK with oscdimg in path, may be required for packer"
                        }
                    ]
                },
                "windows_capabilities" : [
                    {
                        "Name": "OpenSSH.Client*"
                    }
                ]
            },
            "core": {
                "git": {
                },
                "nuget": {
                    "sources": [
                        {
                            "Name": "PSGallery",
                            "InstallationPolicy": "Trusted",
                            "SourceLocation": "https://www.powershellgallery.com/api/v2"
                        }
                    ],
                    "modules": [
                        {
                            "Name": "PSnmap",
                            "MinimumVersion": "1.3.1"
                        },
                        {
                            "Name": "Posh-SSH",
                            "MinimumVersion": "2.2"
                        },
                        {
                            "Name": "Posh-Git", 
                            "MinimumVersion": "1.0.0"
                        },
                        {
                            "Name": "Foil",
                            "MinimumVersion": "0.1.0"
                        },
                        {
                            "Name": "PowershellGet",
                            "MinimumVersion": "2.2.5"
                        },
                        {
                            "Name": "GPRegistryPolicyParser",
                            "MinimumVersion": "1.0.0"
                        }
                    ]
                },
                "choco": {
                    "sources": [
                        {
                            "Name": "Chocolatey",
                            "InstallationPolicy": "Trusted",
                            "SourceLocation": "https://chocolatey.org/api/v2/"
                        }
                    ],
                    "packages": [
                        {
                            "Name": "Git",
                            "Description": "Git client"
                        },
                        {
                            "Name": "Packer",
                            "Description": "Hashicorp Packer"
                        },
                        {
                            "Name": "Terraform",
                            "Description": "Hashicorp Terraform"
                        },
                        {
                            "Name": "windows-adk-oscdimg",
                            "Description": "Windows ADK with oscdimg in path, may be required for packer"
                        }
                    ]
                },
                "windows_capabilities" : [
                    {
                        "Name": "OpenSSH.Client*"
                    }
                ]
            }

        },
        "unix": {
            "git": {
            },
            "nuget": {

            },
            "apt": {

            }
        }
    },
    "logging": {
        "log_to_file":                true,
        "warn_on_to_narrow_console":  true,
        "console_width_threshold":    70,
        "array_first_element_length": 45,
        "left_column_width":          30,
        "post_buffer":                 3,
        "verbose": {
            "foreground_color": "Cyan"
        },
        "debug": {
            "foreground_color": "DarkCyan"
        },
        "warning": {
            "foreground_color": "Yellow"
        },
        "information": {
            "foreground_color": "White"
        },
        "error": {
            "foreground_color": "Red"
        } 
    }
}